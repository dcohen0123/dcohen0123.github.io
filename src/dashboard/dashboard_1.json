{
    "id": "d1",
    "name": "Dashboard 1",
    "inputs": [{
        "id": "institution",
        "name": "Insitution",
        "type": "institution",
        "align": "left"
    }, {
        "id": "datepicker",
        "name": "Date Picker",
        "type": "datepicker",
        "align": "right",
        "value": "ytd",
        "meta": {
            "options": [{
                "key": "1D",
                "value": "1d"
            }, {
                "key": "1W",
                "value": "1w"
            }, {
                "key": "1M",
                "value": "1m"
            }, {
                "key": "1Y",
                "value": "1y"
            }, {
                "key": "YTD",
                "value": "ytd"
            },  {
                "key": "MAX",
                "value": "max"
            }]
        } 
    }, {
        "id": "date",
        "name": "Date",
        "type": "daterange",
        "align": "right"
    }],
    "widgets": [
        {
            "id": "numPatientsCumulativeInstitution",
            "fn": "numPatientsCumulative",
            "name": "# Patients Cumulative",
            "main": {
                "type": "chart",
                "meta": {}
            },
            "pos": {
                "pctX": 0,
                "pctY": 0,
                "pctWidth": 90,
                "pctHeight": 33
            },
            "inputs": [{
                "id": "interval",
                "type": "select",
                "value": "week",
                "meta": {
                    "data": [{
                        "key": "By Day",
                        "value": "day"
                    }, {
                        "key": "By Week",
                        "value": "week"
                    }, {
                        "key": "By Month",
                        "value": "month"
                    }, {
                        "key": "By Year",
                        "value": "year"
                    }]
                }
            },
            {
                "id": "legend",
                "type": "legend"
            }, {
                "id": "maximize",
                "type": "maximize"
            }]
        },
        {
            "id": "numPatients",
            "name": "# Patients",
            "fn": "numPatients",
            "main": {
                "type": "stats"
            },
            "pos": {
                "pctX": 90,
                "pctY": 0,
                "pctWidth": 10,
                "pctHeight": 33
            },
            "inputs": [{
                "id": "maximize",
                "type": "maximize"
            }]
        },
        {   
            "id": "completeness",
            "name": "Completeness",
            "fn": "completeness",
            "pos": {
                "pctX": 0,
                "pctY": 33,
                "pctWidth": 50,
                "pctHeight": 33
            },
            "main": {
                "type": "grid",
                "meta": {
                    "drillCols": ["id", "full_name"],
                    "colDefs": [
                        {"field": "id", "headerName": "ID", "width": 60},
                        {"field": "first_name", "headerName": "First Name", "width": 120},
                        {"field": "last_name", "headerName": "Last Name", "width": 120},
                        {"field": "full_name", "headerName": "Name", "hide": true},
                        {"colId": "completenessBar", "field": "completeness", "headerName": "Completeness", "width": 300, "cellRenderer": "ProgressBar"},
                        {"colId": "completeness", "field": "completeness", "headerName": "Percent", "type": "percent", "width": 100},
                        {"field": "drill_patient", "headerName": "Drill Patient", "width": 150}
                    ]
                }
            },
            "inputs": [{
                "id": "filterPatient",
                "type": "search",
                "meta": {
                    "placeholder": "Patient"
                }
            }, {
                "id": "maximize",
                "type": "maximize"
            }]
        },
        {   
            "id": "patientInfo",
            "name": "Patient Info",
            "fn": "patientInfo",
            "pos": {
                "pctX": 70,
                "pctY": 0,
                "pctWidth": 30,
                "pctHeight": 100
            },
            "hide": true,
            "events": [{
                "type": "WIDGET_DRILL",
                "meta": {
                    "widgetId": "completeness"
                }
            }],
            "main": {
                "type": "grid",
                "meta": {
                    "colDefs": [
                        {"field": "id", "headerName": "ID", "width": 60},
                        {"field": "first_name", "headerName": "First Name", "width": 110},
                        {"field": "last_name", "headerName": "Last Name", "width": 110},
                        {"field": "num_insertions", "headerName": "# Insertions", "width": 120},
                        {"field": "num_correct_insertions", "headerName": "# Correct Insertions", "width": 120},
                        {"field": "confidence", "headerName": "Confidence", "width": 110},
                        {"field": "date_added", "headerName": "Date Created", "type": "date", "width": 120}
                    ]
                }
            },
            "inputs": [{
                "id": "maximize",
                "type": "maximize"
            }, {
                "id": "close",
                "type": "close"
            }]
        },
        {
            "id": "numPatientsGrid",
            "name": "# Patients",
            "fn": "numPatientsGrid",
            "pos": {
                "pctX": 50,    
                "pctY": 33,
                "pctWidth": 50,
                "pctHeight": 33
            },
            "main": {
                "type": "grid",
                "meta": {
                    "colDefs": [
                        {"field": "date", "headerName": "Date"},
                        {"field": "numPatients", "headerName": "# Patients"}
                    ]
                }
            },
            "inputs": [{
                "id": "interval",
                "type": "select",
                "value": "week",
                "meta": {
                    "data": [{
                        "key": "By Day",
                        "value": "day"
                    }, {
                        "key": "By Week",
                        "value": "week"
                    }, {
                        "key": "By Month",
                        "value": "month"
                    }, {
                        "key": "By Year",
                        "value": "year"
                    }]
                }
            }, {
                "id": "maximize",
                "type": "maximize"
            }]
        },
        {
            "id": "hothContact",
            "name": "Hoth Contact",
            "fn": "hothContact",
            "pos": {
                "pctX": 0,
                "pctY": 66,
                "pctWidth": 50,
                "pctHeight": 33
            },
            "main": {
                "type": "hothContact"
            },      
            "inputs": [{
                "id": "maximize",
                "type": "maximize"
            }]
        },
        {
            "id": "institutionContact",
            "name": "Institution Contact",
            "fn": "institutionContact",
            "main": {
                "type": "institutionContact"
            },
            "pos": {
                "pctX": 50,
                "pctY": 66,
                "pctWidth": 50,
                "pctHeight": 33
            }, 
            "inputs": [{
                "id": "maximize",
                "type": "maximize"
            }]
        }
    ]
}